{
  "name": "python-web-crawler",
  "version": "0.4.0",
  "description": "This is simple python web crawler. It is a Python script that crawls any given urls.",
  "main": "index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "offline": "serverless offline --reloadHandler",
    "deploy": "rm -Rf .build && serverless deploy",
    "remove-app": "serverless remove",
    "deploy:prod": "rm -Rf .build && serverless deploy --stage production",
    "remove-app:prod": "serverless remove --stage production"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "serverless": "^3.38.0",
    "serverless-offline": "^13.6.0",
    "serverless-python-requirements": "^6.1.0"
  }
}
